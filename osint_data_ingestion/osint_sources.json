{
  "sources": [
    {
      "name": "Have I Been Pwned",
      "type": "API",
      "url": "https://haveibeenpwned.com/api/v3/breachedaccount/{query}",
      "key_required": true,
      "api_key": "YOUR_HIBP_API_KEY",
      "rate_limit": 2
    },
    {
      "name": "shodan",
      "type": "API",
      "url": "https://api.shodan.io/shodan/host/{query}?key=YOUR_SHODAN_API_KEY",
      "key_required": true,
      "api_key": "YOUR_SHODAN_API_KEY",
      "rate_limit": 1
    },
    {
      "name": "emailrep",
      "type": "API",
      "url": "https://emailrep.io/{query}",
      "key_required": false,
      "rate_limit": 1
    },
    {
      "name": "sherlock",
      "type": "CLI",
      "command": "sherlock {query}",
      "rate_limit": 3
    },
    {
      "name": "onionscan",
      "type": "CLI",
      "command": "onionscan --json {query}",
      "output_format": "json",
      "rate_limit": 5
    },
    {
      "name": "ghunt",
      "type": "CLI",
      "command": "ghunt {query}",
      "output_format": "text",
      "rate_limit": 4
    },
    {
      "name": "masscan",
      "type": "CLI",
      "command": "masscan {target} -p{ports} --rate={rate} -oJ -",
      "params": [
        "target",
        "ports",
        "rate"
      ]
    },
    {
      "name": "theharvester",
      "type": "CLI",
      "command": "theHarvester -d {domain} -b all",
      "description": "Gather emails, subdomains, hosts, and more from public sources",
      "parameters": [
        "domain"
      ]
    },
    {
      "name": "amass_enum", 
      "type":"CLI",
      "command": "amass enum -d {query}",
      "description": "Performs subdomain enumeration using Amass.",
      "parameters": [
        "domain"
      ]
    },
    {
      "name": "amass_intel",
      "type":"CLI",
      "command": "amass intel -d {query}",
      "description": "Gathers intelligence on a domain using Amass.",
      "parameters": [
        "domain"
      ]
    },
    {
      "name": "holehe",
      "type":"CLI",
      "command": "holehe {query}",
      "description": "Check where an email is registered using Holehe."
    },
    {
      "name": "phoneinfoga",
      "type":"CLI",
      "command": "phoneinfoga scan -n {query}",
      "description": "Gather OSINT information about a phone number using PhoneInfoga.",
      "parameters": ["phone_number"]
    },
    {
      "name": "dnstwist",
      "type":"CLI",
      "command": "dnstwist -r -t mx {query}",
      "description": "Detects typosquatting, phishing, and domain permutations for a given domain.",
      "parameters": [
        "domain"
      ]
    },
    {
      "name": "nmap",
      "type":"CLI",
      "command": "nmap -p {query}",
      "description": "Network exploration tool and security/port scanner.",
      "parameters": [
        "target",
        "ports"
      ]
    },
    {
      "name":"exiftool",
      "type": "CLI",
      "command": "exiftool -json file_uploads/{query}"
    }
  ]
}